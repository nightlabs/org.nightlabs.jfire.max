<!DOCTYPE organisation-initialisation
		PUBLIC "-//NightLabs GmbH//DTD Organisation Initialisation 1.0//EN"
		"http://www.nightlabs.de/dtd/organisation-init_1_0.dtd">
<organisation-initialisation>
	<init bean="jfire/ejb/JFireReporting/ReportManager" method="initialise">
		<depends
			module="JFireBase.ear"
			archive="JFireBaseBean.jar"
			bean="jfire/ejb/JFireBaseBean/ConfigManager"
			method="initialise" />
		<depends
			module="JFireScripting.ear"
			archive="JFireScripting.jar"
			bean="jfire/ejb/JFireScripting/ScriptManager"
			method="initialise" />
	</init>
	<init bean="jfire/ejb/JFireReporting/ReportParameterManager" method="initDefaultValueProviders">
		<depends
			module="JFireReporting.ear" 
			archive="JFireReporting.jar"
			bean="jfire/ejb/JFireReporting/ReportManager" />
	</init>
</organisation-initialisation>
