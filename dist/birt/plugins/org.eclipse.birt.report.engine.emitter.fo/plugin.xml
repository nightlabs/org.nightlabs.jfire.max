<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.birt.report.engine.emitter.fo"
   name="BIRT FO Emitter"
   version="1.0.1"
   provider-name="Eclipse.org"
   class="org.eclipse.birt.report.engine.emitter.fo.plugin.FOEmitterPlugin">

   <runtime>
      <library name="foEmitter.jar">
         <export name="*"/>
      </library>
      <library name="lib/commons-logging.jar">
         <export name="*"/>
      </library>
      <library name="lib/fop.jar">
         <export name="*"/>
      </library>
      <library name="lib/avalon-framework-cvs-20020806.jar">
         <export name="*"/>
      </library> 
       <library name="lib/batik.jar">
         <export name="*"/>
      </library>     
   </runtime>
   <requires>
      <import plugin="org.eclipse.birt.core"/>
      <import plugin="org.eclipse.birt.report.model"/>
      <import plugin="org.eclipse.birt.data"/>
      <import plugin="org.eclipse.birt.report.engine"/>
   </requires>
  <extension 
	 	point="org.eclipse.birt.report.engine.emitters"
	 	name = "Engine FO Emitters">

	 	<emitter 
	  		id="org.eclipse.birt.report.engine.emitter.fo"
	  		class="org.eclipse.birt.report.engine.emitter.fo.FOReportEmitter"
	  		format="fo" mimeType="text/html">
	 	</emitter>
        <emitter 
	  		id="org.eclipse.birt.report.engine.emitter.pdf"
	  		class="org.eclipse.birt.report.engine.emitter.fop.PDFReportEmitter"
	  		format="pdf" mimeType="application/pdf">
	 	</emitter>
        <emitter
	  		id="org.eclipse.birt.report.engine.emitter.fop"
	  		class="org.eclipse.birt.report.engine.emitter.fop.FOPReportEmitter"
	  		format="fop" mimeType="text/html"/>
  </extension>  
</plugin>
