<nightlybuild>

  <project name="JFireIssueTracking">
    <!-- source repository is defined in JFireBuildAll/nightly-jfire-server.xml -->
    <ant path="JFireIssueTracking" file="build-nightlabs.xml" target="deploy"/>
    <requires/> <!-- default requires: uses .classpath file -->
    <provides/> <!-- default provides: provides dist/<projectname>.jar in all variations -->
