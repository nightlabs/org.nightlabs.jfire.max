<?xml version="1.0" encoding="UTF-8"?>

<process-definition
  xmlns=""  name="devil.NightLabs.org:DeliveryNote.Customer">
   <start-state name="devil.NightLabs.org:sent">
      <transition name="createReceptionNote" to="devil.NightLabs.org:createdReceptionNote"></transition>
      <transition name="devil.NightLabs.org:deliver" to="devil.NightLabs.org:delivered"></transition>
   </start-state>
   <end-state name="devil.NightLabs.org:createdReceptionNote"></end-state>
   <end-state name="devil.NightLabs.org:booked"></end-state>
   <state name="devil.NightLabs.org:delivered">
      <transition name="devil.NightLabs.org:book" to="devil.NightLabs.org:booked"></transition>
      <transition name="devil.NightLabs.org:bookImplicitely" to="devil.NightLabs.org:bookedImplicitely"></transition>
   </state>
   <node name="devil.NightLabs.org:bookedImplicitely">
      <transition name="" to="devil.NightLabs.org:booked"></transition>
   </node>
</process-definition>