<?xml version="1.0" encoding="UTF-8"?>

<process-definition
  xmlns=""  name="devil.NightLabs.org:Invoice.Vendor">
   <start-state name="devil.NightLabs.org:created">
      <transition name="abort" to="aborted"></transition>
      <transition name="finalize" to="devil.NightLabs.org:finalized"></transition>
   </start-state>
   <state name="devil.NightLabs.org:finalized">
      <transition name="doubt" to="devil.NightLabs.org:doubtful"></transition>
      <transition name="book" to="booked.1"></transition>
      <transition name="pay" to="waitForBooking"></transition>
   </state>
   <end-state name="aborted"></end-state>
   <state name="devil.NightLabs.org:doubtful">
      <transition name="giveup" to="devil.NightLabs.org:uncollectable"></transition>
      <transition name="pay" to="waitForBooking"></transition>
   </state>
   <end-state name="devil.NightLabs.org:uncollectable"></end-state>
   <end-state name="devil.NightLabs.org:paid"></end-state>
   <node name="unbooked">
      <transition name="" to="devil.NightLabs.org:uncollectable"></transition>
   </node>
   <state name="waitForBooking">
      <transition name="book" to="booked.2"></transition>
   </state>
   <node name="booked.2">
      <transition name="" to="devil.NightLabs.org:paid"></transition>
   </node>
   <state name="booked.1">
      <transition name="pay" to="devil.NightLabs.org:paid"></transition>
      <transition name="doubt" to="doubtful-booked"></transition>
   </state>
   <state name="doubtful-booked">
      <transition name="unbook" to="unbooked"></transition>
      <transition name="pay" to="devil.NightLabs.org:paid"></transition>
   </state>
</process-definition>