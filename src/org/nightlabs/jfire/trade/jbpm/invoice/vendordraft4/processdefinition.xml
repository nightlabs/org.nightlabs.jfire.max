<?xml version="1.0" encoding="UTF-8"?>

<process-definition
  xmlns=""  name="devil.NightLabs.org:Invoice.Vendor">
   <start-state name="devil.NightLabs.org:created">
      <transition name="abort" to="aborted"></transition>
      <transition name="finalize" to="devil.NightLabs.org:finalized"></transition>
   </start-state>
   <state name="devil.NightLabs.org:finalized">
      <transition name="" to="fork1"></transition>
   </state>
   <end-state name="aborted"></end-state>
   <state name="devil.NightLabs.org:doubtful">
      <transition name="pay" to="paid"></transition>
      <transition name="giveup" to="isBooked"></transition>
      <transition name="trust" to="waitForPayment"></transition>
   </state>
   <end-state name="devil.NightLabs.org:uncollectable"></end-state>
   <end-state name="devil.NightLabs.org:paid"></end-state>
   <state name="waitForBooking">
      <transition name="book" to="booked"></transition>
   </state>
   <fork name="fork1">
      <transition name="waitForBooking" to="waitForBooking"></transition>
      <transition name="waitForPayment" to="waitForPayment"></transition>
   </fork>
   <state name="waitForPayment">
      <transition name="pay" to="paid"></transition>
      <transition name="doubt" to="devil.NightLabs.org:doubtful"></transition>
   </state>
   <node name="booked">
      <transition name="" to="join1"></transition>
   </node>
   <node name="paid">
      <transition name="" to="join1"></transition>
   </node>
   <join name="join1">
      <transition name="" to="devil.NightLabs.org:paid"></transition>
   </join>
   <decision name="isBooked">
      <transition name="alreadyBooked" to="reversebook"></transition>
      <transition name="notYetBooked" to="devil.NightLabs.org:uncollectable"></transition>
   </decision>
   <node name="reversebook">
      <transition name="" to="devil.NightLabs.org:uncollectable"></transition>
   </node>
</process-definition>