<?xml version="1.0" encoding="UTF-8"?>

<process-definition
  xmlns=""  name="devil.NightLabs.org:Offer.Customer">
   <start-state name="devil.NightLabs.org:sent">
      <transition name="accept" to="devil.NightLabs.org:customerAccepted"></transition>
      <transition name="reject" to="devil.NightLabs.org:customerRejected"></transition>
      <transition name="devil.NightLabs.org:expired" to="devil.NightLabs.org:expired"></transition>
      <transition name="devil.NightLabs.org:revoked" to="devil.NightLabs.org:revoked"></transition>
   </start-state>
   <end-state name="devil.NightLabs.org:customerRejected"></end-state>
   <end-state name="devil.NightLabs.org:customerAccepted"></end-state>
   <end-state name="devil.NightLabs.org:expired"></end-state>
   <end-state name="devil.NightLabs.org:revoked"></end-state>
</process-definition>