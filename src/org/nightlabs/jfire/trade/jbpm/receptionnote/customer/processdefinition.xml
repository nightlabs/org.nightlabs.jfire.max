<?xml version="1.0" encoding="UTF-8"?>

<process-definition
  xmlns=""  name="devil.NightLabs.org:ReceptionNote.Customer">
   <start-state name="devil.NightLabs.org:created">
      <transition name="finalize" to="devil.NightLabs.org:finalized"></transition>
   </start-state>
   <state name="devil.NightLabs.org:finalized">
      <transition name="devil.NightLabs.org:book" to="devil.NightLabs.org:booked"></transition>
   </state>
   <state name="devil.NightLabs.org:booked">
      <transition name="devil.NightLabs.org:send" to="devil.NightLabs.org:sent"></transition>
   </state>
   <node name="devil.NightLabs.org:sent">
      <transition name="end" to="end"></transition>
   </node>
   <end-state name="end"></end-state>
</process-definition>